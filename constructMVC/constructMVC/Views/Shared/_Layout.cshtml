        background: #33CCFF url(@Url.Content("~/Content/images/logo2.png")) no-repeat scroll; padding: 0px; text-align:right  }
.art-content .art-postcontent-0 .layout-item-3 { color: #424242; background: #EDEDED;  }
.art-content .art-postcontent-0 .layout-item-4 { color: #424242; padding-right: 20px;padding-left: 20px;  }
.art-content .art-postcontent-0 .layout-item-5 { margin-top: 5px;  }
.art-content .art-postcontent-0 .layout-item-6 { padding-right: 1px;padding-left: 1px;  }
