<body>
<div id="art-main">
<header class="art-header clearfix">


    <div class="art-shapes">
<h1 class="art-headline" data-left="14.47%">
    EECS
</h1>
<h2 class="art-slogan" data-left="14.72%">Integrated Engineering and Construction</h2>


            </div>

<nav class="art-nav clearfix">
    <ul class="art-hmenu">
        <li>@Html.ActionLink("Home", "Index", "Home", null, new {id = "homeid", @class="tmp" })</li>
        <li>@Html.ActionLink("Services", "Index", "Services", null, new {id = "servicesid", @class="tmp" })</li>
        <li>@Html.ActionLink("Projects", "Index", "Projects", null, new {id = "projectsid", @class="tmp" })</li>
        <li>@Html.ActionLink("Contact Us", "ContactUs", "Home", null, new {id = "contactusid", @class="tmp" })</li>
         <li><a href="#" id="btnlogin"  class="tmp">Login</a> </li>
    </ul>
        </nav>

                    
</header>
<div class="art-sheet clearfix">
       @RenderBody()
 <footer class="art-footer clearfix">
        <div style="border-top:1px solid #dedede; width: 865px"><br>
        © 2011-2012, Encinas Engineering and Construction Services. All Rights Reserved. </div>
    </footer>
</div>
    <p class="art-page-footer">
        &nbsp;</p>
</div>


</body></html>

