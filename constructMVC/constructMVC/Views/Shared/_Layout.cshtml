   <script type="text/javascript" >
       switch (document.title)
       {
           case 'EECS - Home':
               $('#homeid').removeClass('tmp').addClass('active');
               $('#servicesid').removeClass('active');
               $('#projectsid').removeClass('active');
               $('#contactusid').removeClass('active');
               break;
           case 'EECS - Services':
               $('#servicesid').removeClass('tmp').addClass('active');
               $('#homeid').removeClass('active');
               $('#projectsid').removeClass('active');
               $('#contactusid').removeClass('active');
               break;
           case 'EECS - Current Projects':
               $('#projectsid').removeClass('tmp').addClass('active');
               $('#homeid').removeClass('active');
               $('#servicesid').removeClass('active');
               $('#contactusid').removeClass('active');
               break;
           case 'EECS - Contact Us':
               $('#contactusid').removeClass('tmp').addClass('active');
               $('#homeid').removeClass('active');
               $('#projectsid').removeClass('active');
               $('#servicesid').removeClass('active');
               break;

       };
